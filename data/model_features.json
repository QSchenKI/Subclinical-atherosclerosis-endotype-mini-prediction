{
  "model_info": {
    "name": "Atherosclerosis Endotype Classifier",
    "version": "1.0",
    "model_type": "sklearn",
    "algorithm": "Random Forest / Logistic Regression / SVM",
    "training_date": "2024-12-01",
    "model_file": "endotype_model.sav"
  },
  "required_features": [
    {
      "name": "age",
      "category": "demographics",
      "type": "numeric",
      "unit": "years",
      "required": true,
      "validation": {
        "min": 18,
        "max": 120,
        "type": "integer"
      },
      "description": "Patient age in years"
    },
    {
      "name": "sex",
      "category": "demographics",
      "type": "categorical",
      "unit": null,
      "required": true,
      "validation": {
        "options": ["M", "F"],
        "encoding": {
          "M": 1,
          "F": 0
        }
      },
      "description": "Biological sex"
    },
    {
      "name": "il6r",
      "category": "biomarker",
      "type": "numeric",
      "unit": "pg/mL",
      "required": true,
      "validation": {
        "min": 0,
        "max": 10000,
        "type": "float"
      },
      "description": "Interleukin-6 Receptor level",
      "clinical_range": {
        "low": "< 1500",
        "normal": "1500-2500",
        "high": "> 2500"
      }
    },
    {
      "name": "hspb1",
      "category": "biomarker",
      "type": "numeric",
      "unit": "pg/mL",
      "required": true,
      "validation": {
        "min": 0,
        "max": 15000,
        "type": "float"
      },
      "description": "Heat Shock Protein Beta-1 level",
      "clinical_range": {
        "low": "< 2000",
        "normal": "2000-3000",
        "high": "> 3000"
      }
    }
  ],
  "optional_features": [
    {
      "name": "bmi",
      "category": "demographics",
      "type": "numeric",
      "unit": "kg/mÂ²",
      "required": false,
      "validation": {
        "min": 10,
        "max": 60,
        "type": "float"
      },
      "description": "Body Mass Index",
      "imputation": "median"
    },
    {
      "name": "smoking",
      "category": "demographics",
      "type": "categorical",
      "unit": null,
      "required": false,
      "validation": {
        "options": ["never", "former", "current"],
        "encoding": {
          "never": 0,
          "former": 1,
          "current": 2
        }
      },
      "description": "Smoking status",
      "imputation": "mode"
    },
    {
      "name": "tnfr1",
      "category": "biomarker",
      "type": "numeric",
      "unit": "pg/mL",
      "required": false,
      "validation": {
        "min": 0,
        "max": 5000,
        "type": "float"
      },
      "description": "Tumor Necrosis Factor Receptor 1",
      "imputation": "median",
      "clinical_range": {
        "normal": "< 800",
        "elevated": "800-1200",
        "high": "> 1200"
      }
    },
    {
      "name": "crp",
      "category": "biomarker",
      "type": "numeric",
      "unit": "mg/L",
      "required": false,
      "validation": {
        "min": 0,
        "max": 50,
        "type": "float"
      },
      "description": "High-sensitivity C-Reactive Protein",
      "imputation": "median",
      "clinical_range": {
        "low_risk": "< 1",
        "moderate_risk": "1-3",
        "high_risk": "> 3"
      }
    },
    {
      "name": "ldl",
      "category": "biomarker",
      "type": "numeric",
      "unit": "mg/dL",
      "required": false,
      "validation": {
        "min": 0,
        "max": 400,
        "type": "float"
      },
      "description": "LDL Cholesterol",
      "imputation": "median",
      "clinical_range": {
        "optimal": "< 100",
        "near_optimal": "100-129",
        "borderline_high": "130-159",
        "high": "> 160"
      }
    },
    {
      "name": "hdl",
      "category": "biomarker",
      "type": "numeric",
      "unit": "mg/dL",
      "required": false,
      "validation": {
        "min": 0,
        "max": 150,
        "type": "float"
      },
      "description": "HDL Cholesterol",
      "imputation": "median",
      "clinical_range": {
        "low": "< 40 (M), < 50 (F)",
        "normal": "40-60",
        "high": "> 60"
      }
    }
  ],
  "preprocessing": {
    "scaling": "StandardScaler",
    "missing_value_strategy": "median_imputation",
    "categorical_encoding": "ordinal",
    "feature_engineering": [
      "age_squared",
      "il6r_log_transform",
      "hspb1_log_transform",
      "ldl_hdl_ratio"
    ]
  },
  "model_outputs": {
    "primary": {
      "name": "endotype_class",
      "type": "categorical",
      "values": [1, 2, 3, 4],
      "description": "Predicted endotype classification"
    },
    "probabilities": {
      "endotype_1_prob": "Probability of Endotype 1",
      "endotype_2_prob": "Probability of Endotype 2",
      "endotype_3_prob": "Probability of Endotype 3",
      "endotype_4_prob": "Probability of Endotype 4"
    }
  },
  "performance_metrics": {
    "accuracy": 0.78,
    "precision": {
      "endotype_1": 0.82,
      "endotype_2": 0.75,
      "endotype_3": 0.76,
      "endotype_4": 0.81
    },
    "recall": {
      "endotype_1": 0.80,
      "endotype_2": 0.73,
      "endotype_3": 0.77,
      "endotype_4": 0.83
    },
    "f1_score": {
      "endotype_1": 0.81,
      "endotype_2": 0.74,
      "endotype_3": 0.77,
      "endotype_4": 0.82
    },
    "auc_roc": 0.85
  }
}
